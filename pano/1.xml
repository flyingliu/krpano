<krpano version="1.16" onstart="startup();">
  <!-- <security cors="anonymous" /> -->
  <include url="%$mypath%/skin.xml" devices="desktop"/>
  <autorotate enabled="false" waittime="1.5" accel="1.5" speed="5" horizon="15.0" />

  <action name="startup">
  if(startscene === null, copy(startscene,scene[0].name)); loadscene(get(startscene), null, MERGE);
  </action>

  <preview type="grid(cube,16,16,512,0xCCCCCC,0xFFFFFF,0x999999);" />
    
  
</krpano>